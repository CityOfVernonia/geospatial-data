      "url": "https://cityofvernonia.github.io/geospatial-data/floodplain-management/flood-zones.geojson"
    },
    {
      "labelingInfo": [
        {
          "labelExpressionInfo": { "expression": "$feature.name" },
          "repeatLabel": false,
          "symbol": {
            "color": [255, 255, 255],
